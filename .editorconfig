# EditorConfig 配置
# https://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{js,json,vue,html,css}]
indent_size = 2

[*.{jpg,jpeg,png,gif,webp,ico}]
insert_final_newline = false

